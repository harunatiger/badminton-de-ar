<% if @friends.present? %>
  $('#friends_list').html($("<%=j render partial: 'friends_list', locals: { friends: @friends } %>").hide().fadeIn('slow'));
<% end %>
<% if @not_friends.present? %>
  $('#not_friends_list').html($("<%=j render partial: 'not_friends_list', locals: { not_friends: @not_friends } %>").hide().fadeIn('slow'));
<% end %>